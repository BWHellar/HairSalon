<div class="grid">
  <div class="header">
    <div class="home">
      <p><a href="/">Home</a></p>
    </div>
      <h1>stylist</h1>
  </div>
</div>
<div class="aline">
  @if (@Model.Count == 0)
  {
    <h3>No stylist have been added yet!</h3>
  }

  @foreach (var stylist in Model)
  {
    <h3><a href='/stylist/@stylist.GetStylistId()'>@stylist.GetStylistName()</a></h3>
  }
</div>
  <div class="back">
    <p><a href='/stylist/new'>Add New stylist</a></p>
  </div>
